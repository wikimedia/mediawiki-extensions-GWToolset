{
	"name": "GWToolset",
	"version": "0.4.0",
	"author": [
		"Dan Entous"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:GWToolset",
	"descriptionmsg": "gwtoolset-desc",
	"license-name": "GPL-3.0-or-later",
	"type": "media",
	"config": {
